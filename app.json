{
  "expo": {
    "name": "Meme Soundboard",
    "slug": "meme-soundboard",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#98ffcc"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "me.me.soundboard",
      "versionCode": 1,
      "permissions": []
    },
    "description": "Template for Soundboard Apps with Expo",
    "githubUrl": "https://github.com/Redseb/soundboardTemplate"
  }
}
